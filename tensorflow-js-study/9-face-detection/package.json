{
  "name": "parcel-riot-mocktimes-starter",
  "version": "2.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "run-s dist:*",
    "dist-dev": "run-s dist-dev:*",
    "dist:clean": "rimraf dist",
    "dist-dev:clean": "rimraf dist-no-minified",
    "dist:build": "parcel build src/index.html",
    "dist-dev:build": "parcel build src/index.html --out-dir=dist-no-minified --no-minify",
    "lint": "standard --verbose | snazzy",
    "precommit": "lint-staged",
    "start": "am-mocktimes watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "am-mocktimes": "^1.4.0",
    "axios-mock-adapter": "^1.15.0",
    "cross-env": "^5.1.6",
    "husky": "^0.14.3",
    "less": "^3.0.4",
    "lint-staged": "^7.1.2",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.4",
    "parcel-plugin-tag": "^1.0.1",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "snazzy": "^7.1.1",
    "standard": "^10.0.3"
  },
  "standard": {
    "globals": [
      "opts"
    ]
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "axios": "^0.18.0",
